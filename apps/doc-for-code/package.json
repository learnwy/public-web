{
  "name": "doc-for-code",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf ./**/node_modules",
    "start": "NODE_ENV=development && webpack serve --config ./config/webpack.config.js",
    "server": "node server.js",
    "watch": "webpack --watch",
    "build": "export NODE_ENV=production && webpack --config ./config/webpack.config.js",
    "analyze:build": "export NODE_ENV=production NODE_ANALYZE=true && webpack --config ./config/webpack.config.js --profile --json > analyze/stats.json",
    "analyze": "pnpm run analyze:build && webpack-bundle-analyzer analyze/stats.json",
    "new": "modern new",
    "lint": "modern lint"
  },
  "dependencies": {
    "@modern-js/runtime": "2.17.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@modern-js-app/eslint-config": "2.17.0",
    "@modern-js/app-tools": "2.17.0",
    "@modern-js/eslint-config": "2.17.0",
    "@modern-js/tsconfig": "2.17.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@svgr/webpack": "^5.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/jest": "~29.2.4",
    "@types/node": "~16.11.7",
    "@types/react": "~18.0.26",
    "@types/react-dom": "~18.0.10",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "express": "^4.18.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "~8.0.1",
    "json5": "^2.2.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "~13.1.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.5",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "prettier": "~2.8.1",
    "react-refresh-typescript": "^2.0.9",
    "rimraf": "~3.0.2",
    "tailwindcss": "^3.3.2",
    "terser-webpack-plugin": "^5.2.5",
    "toml": "^3.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "~4.9.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.79.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-middleware": "^6.1.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^4.0.2",
